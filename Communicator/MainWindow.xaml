<Window x:Class="Communicator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Communicator"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" Loaded="Window_Loaded" Closed="Window_Closed">
    <Grid>
        <Button  Width="1" Height="1" Background="DarkRed" Content="Click"/>
        <ListBox x:Name="listBox" Width="300" Height="200" Background="Aqua" Margin="13,22,204,97" SelectionChanged="listBox_SelectionChanged" SelectionMode="Multiple"/>
        <Button x:Name="button" Content="Button" Width="105" Height="28" VerticalAlignment="Bottom" Margin="350,0,62,179" Click="button_Click"/>
        <ComboBox x:Name="comboBox" Height="26" Margin="328,22,80,0" VerticalAlignment="Top" Width="109" SelectionChanged="comboBox_SelectionChanged"/>
        <Button x:Name="button_delete" Content="Delete" Width="105" Height="28" VerticalAlignment="Bottom" Margin="45,0,367,39" Click="button_delete_Click"/>


    </Grid>
</Window>
